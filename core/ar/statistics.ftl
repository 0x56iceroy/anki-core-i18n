cards-per-min = {$cards-per-minute} بطاقات/دقيقة
cards = { $cards ->
    [zero] {$cards} بطاقة
    [one] {$cards} بطاقات
    [two] {$cards} بطاقات
    [few] {$cards} بطاقات
    [many] {$cards} بطاقات
   *[other] {$cards} بطاقات
  }
reviews = { $reviews ->
    [zero] {$reviews} مذاكرة
    [one] {$reviews} مذاكرة
    [two] {$reviews} مذاكرات
    [few] {$reviews} مذاكرات
    [many] {$reviews} مذاكرات
   *[other] {$reviews} مذاكرات
  }
