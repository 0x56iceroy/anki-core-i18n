
answer-button-time-months = {$amount} мес.
-time-span-seconds = { $seconds ->
    [one] {$seconds} секунда
   *[other] {$seconds} секунди
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} минута
   *[other] {$minutes} минути
  }
-time-span-hours = { $hours ->
    [one] {$hours} час
   *[other] {$hours} часа
  }
-time-span-days = { $days ->
    [one] {$days} ден
   *[other] {$days} дни
  }
-time-span-months = { $months ->
    [one] {$months} месец
   *[other] {$months} месеца
  }
-time-span-years = { $years ->
    [one] {$years} година
   *[other] {$years} години
  }
