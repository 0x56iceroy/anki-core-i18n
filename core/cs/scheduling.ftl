
answer-button-time-minutes = {$amount}min

answer-button-time-hours = {$amount}hod

answer-button-time-days = {$amount}dnů

answer-button-time-months = {$amount}měs

answer-button-time-years = {$amount}r
-time-span-seconds = { $seconds ->
    [one] {$seconds} sekunda
    [few] {$seconds} sekundy
    [many] {$seconds} sekund
   *[other] {$seconds} sekund
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minuta
    [few] {$minutes} minuty
    [many] {$minutes} minut
   *[other] {$minutes} minut
  }
-time-span-hours = { $hours ->
    [one] {$hours} hodina
    [few] {$hours} hodiny
    [many] {$hours} hodin
   *[other] {$hours} hodin
  }
-time-span-days = { $days ->
    [one] {$days} den
    [few] {$days} dny
    [many] {$days} dní
   *[other] {$days} dní
  }
-time-span-months = { $months ->
    [one] {$months} měsíc
    [few] {$months} měsíce
    [many] {$months} měsíců
   *[other] {$months} měsíců
  }
-time-span-years = { $years ->
    [one] {$years} rok
    [few] {$years} roky
    [many] {$years} let
   *[other] {$years} let
  }
