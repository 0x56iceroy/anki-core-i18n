
answer-button-time-seconds = {$amount} s

answer-button-time-minutes = {$amount} m

answer-button-time-hours = {$amount} h

answer-button-time-days = {$amount} t

answer-button-time-months = {$amount} mo

answer-button-time-years = {$amount} j
-time-span-seconds = { $seconds ->
    [one] {$seconds} sekundo
   *[other] {$seconds} sekundoj
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minuto
   *[other] {$minutes} minutoj
  }
-time-span-hours = { $hours ->
    [one] {$hours} horo
   *[other] {$hours} horoj
  }
-time-span-days = { $days ->
    [one] {$days} tago
   *[other] {$days} tagoj
  }
-time-span-months = { $months ->
    [one] {$months} monato
   *[other] {$months} monatoj
  }
-time-span-years = { $years ->
    [one] {$years} jaro
   *[other] {$years} jaroj
  }
