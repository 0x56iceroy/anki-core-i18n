time-span-seconds = { $amount ->
    [one] {$amount} sekund
   *[other] {$amount} sekundit
  }
time-span-minutes = { $amount ->
    [one] {$amount} minut
   *[other] {$amount} minutit
  }
time-span-hours = { $amount ->
    [one] {$amount} tund
   *[other] {$amount} tundi
  }
time-span-days = { $amount ->
    [one] {$amount} päev
   *[other] {$amount} päeva
  }
time-span-months = { $amount ->
    [one] {$amount} kuu
   *[other] {$amount} kuud
  }
time-span-years = { $amount ->
    [one] {$amount} aasta
   *[other] {$amount} aastat
  }
