    [one] {$seconds} sekunti
   *[other] {$seconds} sekuntia
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minuutti
   *[other] {$minutes} minuuttia
  }
-time-span-hours = { $hours ->
    [one] {$hours} tunti
   *[other] {$hours} tuntia
  }
-time-span-days = { $days ->
    [one] {$days} päivä
   *[other] {$days} päivää
  }
-time-span-months = { $months ->
    [one] {$months} kuukausi
   *[other] {$months} kuukautta
  }
-time-span-years = { $years ->
    [one] {$years} vuosi
   *[other] {$years} vuotta
  }
