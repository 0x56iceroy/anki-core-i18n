
answer-button-time-seconds = {$amount} mp

answer-button-time-minutes = {$amount} p

answer-button-time-hours = {$amount} ó

answer-button-time-days = {$amount} n

answer-button-time-months = {$amount} hó

answer-button-time-years = {$amount} év
-time-span-seconds = { $seconds ->
    [one] {$seconds} mp
   *[other] {$seconds} mp
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} perc
   *[other] {$minutes} perc
  }
-time-span-hours = { $hours ->
    [one] {$hours} óra
   *[other] {$hours} óra
  }
-time-span-days = { $days ->
    [one] {$days} nap
   *[other] {$days} nap
  }
-time-span-months = { $months ->
    [one] {$months} hónap
   *[other] {$months} hónap
  }
-time-span-years = { $years ->
    [one] {$years} év
   *[other] {$years} év
  }
