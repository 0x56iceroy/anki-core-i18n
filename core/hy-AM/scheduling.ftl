
answer-button-time-seconds = {$amount} վ.

answer-button-time-minutes = {$amount} ր.

answer-button-time-hours = {$amount} ժ.

answer-button-time-days = {$amount} օր

answer-button-time-months = {$amount} ամ.

answer-button-time-years = {$amount} տ.
-time-span-seconds = { $seconds ->
    [one] {$seconds} վայրկյանում
   *[other] {$seconds} վայրկյանում
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} րոպեում
   *[other] {$minutes} րոպեում
  }
-time-span-hours = { $hours ->
    [one] {$hours} ժամում
   *[other] {$hours} ժամում
  }
-time-span-days = { $days ->
    [one] {$days} օրում
   *[other] {$days} օրում
  }
-time-span-months = { $months ->
    [one] {$months} ամսում
   *[other] {$months} ամսում
  }
-time-span-years = { $years ->
    [one] {$years} տարում
   *[other] {$years} տարում
  }
