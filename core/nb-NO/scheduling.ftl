
answer-button-time-seconds = {$amount} sek

answer-button-time-minutes = {$amount} min

answer-button-time-hours = {$amount} tim.

answer-button-time-days = {$amount} dag

answer-button-time-months = {$amount}md.

answer-button-time-years = {$amount} år
-time-span-seconds = { $seconds ->
    [one] {$seconds} sekund
   *[other] {$seconds} sekunder
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minutt
   *[other] {$minutes} minutter
  }
-time-span-hours = { $hours ->
    [one] {$hours} time
   *[other] {$hours} timer
  }
-time-span-days = { $days ->
    [one] {$days} dag
   *[other] {$days} dager
  }
-time-span-months = { $months ->
    [one] {$months} måned
   *[other] {$months} måneder
  }
-time-span-years = { $years ->
    [one] {$years} år
   *[other] {$years} år
  }
