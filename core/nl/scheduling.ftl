-time-span-seconds = { $seconds ->
    [one] {$seconds} seconde
   *[other] {$seconds} seconden
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minuut
   *[other] {$minutes} minuten
  }
-time-span-hours = { $hours ->
    [one] {$hours} uur
   *[other] {$hours} uur
  }
-time-span-days = { $days ->
    [one] {$days} dag
   *[other] {$days} dagen
  }
-time-span-months = { $months ->
    [one] {$months} maand
   *[other] {$months} maanden
  }
-time-span-years = { $years ->
    [one] {$years} jaar
   *[other] {$years} jaar
  }
