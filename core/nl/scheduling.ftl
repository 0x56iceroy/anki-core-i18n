time-span-seconds = { $amount ->
    [one] {$amount} seconde
   *[other] {$amount} seconden
  }
time-span-minutes = { $amount ->
    [one] {$amount} minuut
   *[other] {$amount} minuten
  }
time-span-hours = { $amount ->
    [one] {$amount} uur
   *[other] {$amount} uur
  }
time-span-days = { $amount ->
    [one] {$amount} dag
   *[other] {$amount} dagen
  }
time-span-months = { $amount ->
    [one] {$amount} maand
   *[other] {$amount} maanden
  }
time-span-years = { $amount ->
    [one] {$amount} jaar
   *[other] {$amount} jaar
  }
