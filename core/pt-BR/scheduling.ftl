
answer-button-time-seconds = {$amount}s

answer-button-time-minutes = {$amount}min(s)

answer-button-time-hours = {$amount}h

answer-button-time-days = {$amount}dia(s)

answer-button-time-months = {$amount}mês(meses)

answer-button-time-years = {$amount}a
-time-span-seconds = { $seconds ->
    [one] {$seconds} segundo
   *[other] {$seconds} segundos
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minuto
   *[other] {$minutes} minutos
  }
-time-span-hours = { $hours ->
    [one] {$hours} hora
   *[other] {$hours} horas
  }
-time-span-days = { $days ->
    [one] {$days} dia
   *[other] {$days} dias
  }
-time-span-months = { $months ->
    [one] {$months} mês
   *[other] {$months} meses
  }
-time-span-years = { $years ->
    [one] {$years} ano
   *[other] {$years} anos
  }
