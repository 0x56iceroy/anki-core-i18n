
answer-button-time-seconds = {$amount} с

answer-button-time-minutes = {$amount} мин.

answer-button-time-hours = {$amount} ч.

answer-button-time-days = {$amount} дн.

answer-button-time-months = {$amount} мес.

answer-button-time-years = {$amount} г.
-time-span-seconds = { $seconds ->
    [one] {$seconds} секунду
    [few] {$seconds} секунды
   *[other] {$seconds} секунд
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} минуту
    [few] {$minutes} минуты
   *[other] {$minutes} минут
  }
-time-span-hours = { $hours ->
    [one] {$hours} час
    [few] {$hours} часа
   *[other] {$hours} часов
  }
-time-span-days = { $days ->
    [one] {$days} день
    [few] {$days} дня
   *[other] {$days} дней
  }
-time-span-months = { $months ->
    [one] {$months} месяц
    [few] {$months} месяца
   *[other] {$months} месяцев
  }
-time-span-years = { $years ->
    [one] {$years} год
    [few] {$years} года
   *[other] {$years} лет
  }
