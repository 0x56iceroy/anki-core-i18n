
answer-button-time-minutes = {$amount}min

answer-button-time-hours = {$amount}hod

answer-button-time-months = {$amount}mes

answer-button-time-years = {$amount}r
-time-span-seconds = { $seconds ->
    [one] {$seconds} sekúnd
    [few] {$seconds} sekunda
    [many] {$seconds} sekundy
   *[other] {$seconds} sekundy
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} minút
    [few] {$minutes} minútu
    [many] {$minutes} minúty
   *[other] {$minutes} minúty
  }
-time-span-hours = { $hours ->
    [one] {$hours} hodín
    [few] {$hours} hodina
    [many] {$hours} hodiny
   *[other] {$hours} hodiny
  }
-time-span-days = { $days ->
    [one] {$days} dní
    [few] {$days} deň
    [many] {$days} dni
   *[other] {$days} dni
  }
-time-span-months = { $months ->
    [one] {$months} mesiacov
    [few] {$months} mesiac
    [many] {$months} mesiace
   *[other] {$months} mesiace
  }
-time-span-years = { $years ->
    [one] {$years} rokov
