
answer-button-time-seconds = {$amount} с.

answer-button-time-minutes = {$amount} мин.

answer-button-time-hours = {$amount} ч.

answer-button-time-days = {$amount} д.

answer-button-time-months = {$amount} мес.

answer-button-time-years = {$amount} г.
-time-span-seconds = { $seconds ->
    [one] {$seconds} секунда
    [few] {$seconds} секунде
   *[other] {$seconds} секунди
  }
-time-span-minutes = { $minutes ->
    [one] {$minutes} минут
    [few] {$minutes} минута
   *[other] {$minutes} минута
  }
-time-span-hours = { $hours ->
    [one] {$hours} сат
    [few] {$hours} сата
   *[other] {$hours} сати
  }
-time-span-days = { $days ->
    [one] {$days} дан
    [few] {$days} дана
   *[other] {$days} дана
  }
-time-span-months = { $months ->
    [one] {$months} месец
    [few] {$months} месеца
   *[other] {$months} месеци
  }
-time-span-years = { $years ->
    [one] {$years} година
    [few] {$years} године
   *[other] {$years} година
  }
